{
  "name": "angular-boilerplate",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "ng": "ng --max_old_space_size=7000 ./node_modules/@angular/cli/bin/ng",
    "ng-high-memory": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --prod --aot",
    "postinstall": "npx husky install",
    "start": "ng serve",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test:unit": "ng test",
    "test:e2e": "ng e2e",
    "test:coverage": "ng test --coverage",
    "lint": "ng lint",
    "analyze": "webpack-bundle-analyzer dist/angular-boilerplate/stats.json",
    "release": "standard-version",
    "prebuild": "npm --no-git-tag-version version patch"
  },
  "dependencies": {
    "@angular/animations": "13.0.2",
    "@angular/cdk": "^12.2.6",
    "@angular/common": "13.0.2",
    "@angular/compiler": "13.0.2",
    "@angular/core": "13.0.2",
    "@angular/forms": "13.0.2",
    "@angular/platform-browser": "13.0.2",
    "@angular/platform-browser-dynamic": "13.0.2",
    "@angular/router": "13.0.2",
    "@ngneat/transloco": "^2.23.2",
    "@ngx-pwa/local-storage": "^13.0.2",
    "@taiga-ui/addon-charts": "^2.16.3",
    "@taiga-ui/addon-commerce": "^2.16.3",
    "@taiga-ui/addon-doc": "2.16.3",
    "@taiga-ui/addon-editor": "2.16.3",
    "@taiga-ui/addon-mobile": "^2.16.3",
    "@taiga-ui/addon-table": "^2.16.3",
    "@taiga-ui/addon-tablebars": "^2.16.3",
    "@taiga-ui/cdk": "2.16.3",
    "@taiga-ui/core": "2.16.3",
    "@taiga-ui/icons": "2.16.3",
    "@taiga-ui/kit": "2.16.3",
    "@tinkoff/ng-dompurify": "3.0.0",
    "dompurify": "2.2.9",
    "g": "^2.0.1",
    "http-server": "^14.0.0",
    "ngx-spinner": "^12.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^12.1.1",
    "rxjs": "7.4.0",
    "rxjs-compat": "^6.6.7",
    "standard-version": "^9.3.2",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.0.0",
    "@angular-devkit/build-angular": "13.0.3",
    "@angular-eslint/builder": "12.4.1",
    "@angular-eslint/eslint-plugin": "12.4.1",
    "@angular-eslint/eslint-plugin-template": "12.4.1",
    "@angular-eslint/schematics": "12.4.1",
    "@angular-eslint/template-parser": "12.4.1",
    "@angular/cli": "13.0.3",
    "@angular/compiler-cli": "13.0.2",
    "@briebug/cypress-schematic": "5.0.1",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@cypress/webpack-preprocessor": "5.9.1",
    "@types/dompurify": "2.2.3",
    "@types/jasmine": "3.9.0",
    "@types/jest": "27.0.1",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "angular-cli-ghpages": "^1.0.0",
    "autoprefixer": "10.3.4",
    "cypress": "8.3.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-angular": "4.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "26.6.3",
    "postcss": "8.3.6",
    "prettier": "2.4.0",
    "prettier-eslint": "^8.2.2",
    "tailwindcss": "^2.2.15",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.4",
    "webpack-bundle-analyzer": "4.4.2"
  }
}
